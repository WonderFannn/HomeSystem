<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_homesettingactivity_main"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/iv_backbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="15dp"
        android:src="@drawable/ic_homesettingactivity_return" />

    <ExpandableListView
        android:id="@+id/expandableListView1"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/iv_backbutton"
        android:layout_toRightOf="@+id/iv_backbutton"
        android:divider="@drawable/bg_homesettingactivity_divider"
        android:groupIndicator="@null"
	    android:padding="15dp"
	    android:paddingRight="30dp" >

    </ExpandableListView>

</RelativeLayout>
